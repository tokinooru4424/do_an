{
  "apps": [
    {
      "name": "MQ Secure Server System",
      "script": "./dist/server/index.js",
      "watch": false,
      "env_development": {
        "NODE_ENV": "development",
        "MODE": "dev-server",
        "DEBUG": "@ngochipx:mqSecureServer*"
      },
      "env": {
        "NODE_ENV": "production"
      },
      "env_DEBUG": {
        "NODE_ENV": "production",
        "DEBUG": "@ngochipx:mqSecureServer*"
      },
      "ignore_watch": [
        "node_modules",
        "logs",
        "bower_components",
        ".tmp",
        ".vscode",
        "views",
        "routes",
        "public",
        ".next"
      ],
      "error_file": "./logs/error.log",
      "out_file": "./logs/out.log",
      "pid_file": "./logs/app.pid",
      "log_date_format": "YYYY-MM-DD HH:mm Z"
    }
  ]
}